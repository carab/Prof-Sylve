{
  "name": "prof-sylve",
  "private": true,
  "version": "1.2.0",
  "description": "Prof. Sylve's Living Dex, for the most hardcore trainers.",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.png ./src/manifest.json ./src/icon-144.png ./src/icon-192.png ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "firebase:deploy": "firebase deploy",
    "firebase:login": "firebase login"
  },
  "repository": "https://github.com/carab/Prof-Sylve",
  "keywords": [
    "pokemon",
    "pokedex",
    "livingdex"
  ],
  "author": {
    "name": "Camille Rabatel",
    "email": "camille.rabatel+github@gmail.com"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.10.1",
    "firebase-tools": "^3.5.0",
    "glob": "^7.1.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "null-loader": "^0.1.1",
    "offline-plugin": "^4.6.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss": "^5.2.16",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "firebase": "^3.7.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.1",
    "prof-sylve-sprites": "^1.1.0",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-event-listener": "^0.4.2",
    "react-helmet": "^4.0.0",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.3",
    "react-responsive": "^1.2.6",
    "react-router-dom": "^4.0.0",
    "react-swipe": "^5.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "react-virtualized": "^9.2.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "swipe-js-iso": "^2.0.3"
  }
}
